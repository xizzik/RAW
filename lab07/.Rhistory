library(tidyverse)
rmunif <- function(n){
temp <- tibble(a = runif(n), b = runif(n), c = runif(n), d = runif(n)) %>% rowwise() %>% mutate(max=max(a,b,c,d)) %>% select(3) %>% ungroup()
return(temp[[1]])
}
hist(rmunif)
